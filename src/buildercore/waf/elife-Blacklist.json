{
  "Name": "elife-Blacklist",
  "Priority": 15,
  "Action": {
    "Block": {}
  },
  "VisibilityConfig": {
    "SampledRequestsEnabled": true,
    "CloudWatchMetricsEnabled": true,
    "MetricName": "elife-Blacklist"
  },
  "Statement": {
    "OrStatement": {
      "Statements": [
        {
          "LabelMatchStatement": {
            "Scope": "LABEL",
            "Key": "awswaf:managed:aws:bot-control:bot:name:barkrowler"
          }
        },
        {
          "LabelMatchStatement": {
            "Scope": "LABEL",
            "Key": "awswaf:managed:aws:bot-control:bot:name:megaindex"
          }
        },
        {
          "LabelMatchStatement": {
            "Scope": "LABEL",
            "Key": "awswaf:managed:aws:bot-control:bot:name:alexabot"
          }
        },
        {
          "LabelMatchStatement": {
            "Scope": "LABEL",
            "Key": "awswaf:managed:aws:bot-control:bot:name:serpstatbot"
          }
        },
        {
          "LabelMatchStatement": {
            "Scope": "LABEL",
            "Key": "awswaf:managed:aws:bot-control:bot:name:ahrefsbot"
          }
        },
        {
          "LabelMatchStatement": {
            "Scope": "LABEL",
            "Key": "awswaf:managed:aws:bot-control:bot:name:mauibot"
          }
        },
        {
          "RegexMatchStatement": {
            "FieldToMatch": {
              "SingleHeader": {
                "Name": "User-Agent"
              }
            },
            "TextTransformations": [
              {
                "Type": "LOWERCASE",
                "Priority": 0
              }
            ],
            "RegexString": "^.+(blexbot|neevabot|headlesschrome|python-urllib|python-requests|aiohttp|seostar|seznambot|seekport|applebot|centuryb\\.o\\.t|dotbot|scrapy|ultimate_sitemap_parser|placeholder|bdbot|sentibot).+"
          }
        }
      ]
    }
  }
}
